<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title>Kamil`s blog</title>
    <link rel="stylesheet" href="./style.css">
  </head>
  <body>
    <h1>What happened today?</h1>
    <div class="header">
      <div class="image-container">
      </div>


      <div class="message">
        <textarea></textarea>
      </div>

      <div class="button">
        <p id="button-text">PUBLISH >></p>
      </div>
    </div>


    <div class="container">
      <ul id="content-list">
        <% dataArray.forEach(function(obj) { %>
          <li>
            <p id="text"><%= obj.data %></p>
            <p id="likes"><%= obj.likes %></p>
          </li>
        <% })%>
      </ul>
    </div>

    <script src="./index.js" type="application/javascript"></script>
  </body>
</html>
